-#ifdef HAVE_SHA256
-    static int reading_sha256sums = 0;
-#endif
-
-    switch (*line) {
-    case 'A':
-        if (is_field("Architectures", line)) {
-            release->architectures = parse_list(line, &release->architectures_count,
-                    ' ', 0);
-        }
-        break;
-
-    case 'C':
-        if (is_field("Codename", line)) {
-            release->name = parse_simple("Codename", line);
